{
  "name": "scirpus",
  "version": "0.6.0",
  "description": "Generate JavaScript AST from Cirru Grammer",
  "main": "lib/operations.js",
  "scripts": {
    "clean": "rm -rfv dist/*",
    "watch": "yarn html && webpack-dev-server --config webpack.dev.cirru",
    "release": "yarn clean && webpack --config webpack.release.cirru && yarn release-html && mkdir dist/test && cp -rv test/cirru dist/test/",
    "html": "cirru-script template.cirru",
    "release-html": "env=release cirru-script template.cirru",
    "up": "yarn release && tiye-up",
    "test": "cirruscript test/test.cirru"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "keywords": [
    "cirru"
  ],
  "devDependencies": {
    "@babel/generator": "^7.8.0",
    "@babel/parser": "^7.8.0",
    "assets-webpack-plugin": "^3.9.10",
    "cirru-parser": "^0.10.2",
    "cirru-script": "^0.6.0-a1",
    "cirru-script-loader": "^0.5.4",
    "css-loader": "^3.4.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "fast-deep-equal": "^3.1.1",
    "file-loader": "^5.0.2",
    "http-server": "^0.12.0",
    "immutable": "^3.7.5",
    "jsondiffpatch": "^0.4.1",
    "sort-json": "^2.0.0",
    "stir-template": "^0.2.2",
    "style-loader": "^1.1.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-hud": "^0.1.2"
  },
  "dependencies": {
    "type-of": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cirru/scirpus.git"
  },
  "bugs": {
    "url": "https://github.com/Cirru/scirpus/issues"
  },
  "homepage": "https://github.com/Cirru/scirpus"
}
